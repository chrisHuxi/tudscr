\relax

\metrics

\inputmtx{fdd-fix}

\setcommand\mapoperatorglyph#1#2#3{%
  \mapsymbglyph{#1text}{#2}%
  \mapsymbglyph{#1display}{#3}%
}
\setcommand\copyoperatorglyph#1#2{%
  \copysymbglyph{#1text}{#1}%
  \mapsymbglyph{#1display}{#2}%
}
\setcommand\copysymbglyph#1#2{%
  \copyglyph{#1}{#2}%
}
\setcommand\mapsymbglyph#1#2{%
  \mapglyph{#1}{#2}%
}
\setcommand\mapextensibleglyph#1#2#3#4#5{%
  \mapsymbglyph{#1big}{#2}%
  \mapsymbglyph{#1Big}{#3}%
  \mapsymbglyph{#1bigg}{#4}%
  \mapsymbglyph{#1Bigg}{#5}%
}
\setcommand\mapvariableglyph#1#2#3#4{%
  \mapsymbglyph{#1tp}{#2}%
  \mapsymbglyph{#1mid,#1ex}{#3}%
  \mapsymbglyph{#1bt}{#4}%
  \resetglyph{#1ex}
    \glyph{#1ex}{1000}
    \resetdepth{\mul{2}{\depth{#1ex}}}
  \endresetglyph
}
\setcommand\mapaccentglyph#1#2#3#4{%
  \mapsymbglyph{#1wide}{#2}%
  \mapsymbglyph{#1wider}{#3}%
  \mapsymbglyph{#1widest}{#4}%
  
}
\setcommand\setintegralitalics#1#2{%
  \resetglyph{integraltext}%
    \glyph{integraltext}{1000}%
    \resetitalic{#1}%
  \endresetglyph%
  \resetglyph{contintegraltext}%
    \glyph{contintegraltext}{1000}%
    \resetitalic{#1}%
  \endresetglyph%
  \resetglyph{integraldisplay}%
    \glyph{integraldisplay}{1000}%
    \resetitalic{#2}%
  \endresetglyph%
  \resetglyph{contintegraldisplay}%
    \glyph{contintegraldisplay}{1000}%
    \resetitalic{#2}%
  \endresetglyph%
}

